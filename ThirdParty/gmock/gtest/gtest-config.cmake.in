# - Config file for the gtest package
# It defines the following variables
#  GTEST_INCLUDE_DIR - include directory
#  GTEST_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(GTEST_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(GTEST_INCLUDE_DIR "@CONFIG_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${GTEST_CMAKE_DIR}/gtest-targets.cmake")

# These are IMPORTED targets created by gtest-targets.cmake
set(GTEST_LIBRARIES "@EXPORT_TARGETS@")
